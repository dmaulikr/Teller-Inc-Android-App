<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_blocked"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/com_facebook_button_border_color_focused"
    tools:context="com.teller.kwill.teller.BlockedActivity">

    <ScrollView
        android:id="@+id/kjkh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollIndicators="right"
        tools:scrollbars="vertical">

        <LinearLayout
            android:id="@+id/kko"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:scrollbars="vertical" />
    </ScrollView>

    <TextView
        android:id="@+id/textView277"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/button182"
        android:layout_marginEnd="8dp"
        android:layout_toStartOf="@+id/button182"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ListView
        android:id="@+id/loprr"
        android:layout_width="400dp"
        android:layout_height="350dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button182"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:background="@color/com_facebook_button_background_color_selected"
        android:text="Unblock"
        android:layout_below="@+id/jhk"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="24dp" />

    <SearchView
        android:id="@+id/jhk"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp" />

</RelativeLayout>