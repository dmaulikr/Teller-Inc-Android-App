<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_notification"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/com_facebook_button_border_color_focused"
    tools:context="com.teller.kwill.teller.NotificationActivity">

    <ScrollView
        android:id="@+id/iee"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:scrollIndicators="right">

        <LinearLayout
            android:id="@+id/qqq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:scrollbars="vertical" />
    </ScrollView>

    <TextView
        android:id="@+id/textView90"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="26dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="20sp" />

    <ListView
        android:id="@+id/lopm"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/imageVie"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="75dp"
        app:srcCompat="@drawable/com_facebook_auth_dialog_header_background"
        android:layout_below="@+id/textView90"
        android:layout_alignParentStart="true" />

    <VideoView
        android:id="@+id/videoViewm"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_below="@+id/imageVie"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/editText27"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView90"
        android:ems="10"
        android:inputType="textPersonName"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="7dp"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="16dp"
        android:layout_toEndOf="@+id/textView5"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button9"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView90"
        android:layout_marginEnd="23dp"
        android:background="@drawable/com_facebook_tooltip_blue_topnub" />

    <Button
        android:id="@+id/button14"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/iee"
        android:layout_alignStart="@+id/button9"
        android:background="@android:drawable/ic_delete" />

    <Button
        android:id="@+id/button13"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:drawable/ic_delete"
        android:layout_marginBottom="24dp"
        android:layout_alignBottom="@+id/videoViewm"
        android:layout_toEndOf="@+id/videoViewm" />

    <Button
        android:id="@+id/button15"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/videoViewm"
        android:layout_toEndOf="@+id/imageVie"
        android:background="@android:drawable/ic_delete" />

</RelativeLayout>